                <ref bean="jobSMART"/>
                <ref bean="jobTIGRFAM"/>

                <ref bean="jobSuperFamily-1.75"/>
                <ref bean="jobTMHMM"/>
            </list>
        </property>
        <property name="baseDirectoryTemporaryFiles" value="${temporary.file.directory}"/>
    </bean>


