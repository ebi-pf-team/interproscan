<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <bean id="parser" class="uk.ac.ebi.interpro.scan.web.biomart.AnalyseBioMartQueryResult">
        <property name="reader" ref="reader" />
        <property name="resource" ref="file" />
    </bean>

    <bean id="reader" class="uk.ac.ebi.interpro.scan.web.biomart.BioMartQueryResourceReader"/>

    <bean id="file" class="org.springframework.core.io.ClassPathResource">
        <constructor-arg value="data/BioMartQueryResult.dat"/>
    </bean>

 </beans>
